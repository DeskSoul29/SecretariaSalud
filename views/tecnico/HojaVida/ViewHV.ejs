      var nav3 = document.getElementById("collapseThree");
      var nav4 = document.getElementById("show3");
      nav.className += "  active";
      nav3.className += "  show";
      nav4.className += "  active";

      if ("<%= consultHV.grupo %>" == "VEHÍCULOS TRANSPORTADORES DE ALIMENTOS Y BEBIDAS") {
        document.getElementById("placaDiv").removeAttribute("style")
      } 
    });

    $('table.display tfoot th').each(function() {
      var title = $(this).text();
      $(this).html('<input type="text" placeholder="Search ' + title + '" />');
    });
    var table = $('table.display').DataTable({
      order: [
        [1, 'desc']
      ],
      scrollY: true,
      scrollX: true,
      scrollCollapse: true,
      language: {
        search: "Buscar:",
        info: "Mostrando _START_ elementos a _END_ de _TOTAL_ entradas",
        zeroRecords: "Sin Datos en esta Tabla",
        infoFiltered: " - filtrado desde _MAX_ entradas",
        lengthMenu: "Mostrando _MENU_ entradas",
        paginate: {
          previous: "Anterior",
          next: "Siguiente",
        },
      },
      dom: 'Bfrtlip',
      buttons: [{
        extend: 'excel',
        filename: 'ANTIRRABICA_' + new Date().toJSON().substring(0, 10),
        autoFilter: true,
        sheetName: 'ANTIRRABICA ANIMAL',
      }],
    });
    table.columns().every(function() {
      var that = this;
      $('input', this.footer()).on('keyup change', function() {
        that
          .search(this.value)
          .draw();
      });
    });
    </script>

