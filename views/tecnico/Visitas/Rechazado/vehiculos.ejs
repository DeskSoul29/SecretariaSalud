<html lang="es">

<head>
  <%- include('../../../tools/head'); %>
  <style>
    .btn-upload {
      padding: 10px 20px;
      margin-left: 10px;
    }

    .upload-input-group {
      margin-bottom: 10px;
    }

    .input-group>.custom-select:not(:last-child),
    .input-group>.form-control:not(:last-child) {
      height: 45px;
    }
  </style>
</head>

<body id="page-top">
  <!-- Page Wrapper -->
  <div id="wrapper">
    <!-- Sidebar -->
    <%- include('../../../tools/sidebarTec'); %>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <%- include('../../../tools/topbar'); %>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">
          <div class="container">
            <div class="card o-hidden border-0 shadow-lg">
              <div class="card-body p-0">
                <nav style="--bs-breadcrumb-divider: '>'" aria-label="breadcrumb">
                  <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                      <a href="/tecnico/Consolidaciones/Ver">Consolidaciones</a>
                    </li>
                    <li class="breadcrumb-item">
                      <a href="/tecnico/Consolidaciones/Ver/Vehiculos">Vehiculos</a>
                    </li>
                    <li class="breadcrumb-item active" aria-current="page">
                      Corregir
                    </li>
                  </ol>
                </nav>
                <!-- Nested Row within Card Body -->
                <div class="p-5">
                  <div class="text-center">
                    <h1 class="h4 text-gray-900 mb-4">VEHICULOS</h1>
                  </div>

                  <form onsubmit="validConsolidacion(event)" action="/tecnico/Consolidaciones/Rechazado/Vehiculos/<%= consolidacion._id %>" method="POST" autocomplete="off">
                    <div class="form-group row">
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="criterioProf">Criterio</label>
                        <input type="text" class="form-control form-control-user" value="Rechazado" readonly>
                      </div>
                      <div class="col-sm-9" id="divMotivo">
                        <label class="mr-sm-2" for="motivo">Motivo</label>
                        <textarea maxlength="270" rows="2" class="form-control form-control-user" readonly><%= consRech.respuesta.motivo %></textarea>
                      </div>
                    </div>

                    <!-- Divider -->
                    <hr class="sidebar-divider" style="border-top: 1px solid rgb(0 0 0)" />

                    <div class="form-row align-items-center mb-sm-2">
                      <div class="col my-1">
                        <label class="mr-sm-2" for="provinciaID">Provincia</label>
                        <input type="text" class="form-control form-control-user" id="provinciaID" name="provincia" value="<%= user.provincia %>" readonly>
                      </div>
                      <div class="col my-1">
                        <label class="mr-sm-2" for="muniSelect">Municipio</label>
                        <input type="text" class="form-control form-control-user" id="muniSelect" name="municipio" value="<%= consolidacion.municipio %>" readonly>
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-8 mb-3 mb-sm-0">
                        <label class="mr-sm-2" for="grupEsta">Grupo Del
                          Establecimiento</label>
                        <input type="text" value="<%= consolidacion.grupo %>" class="form-control form-control-user" id="grupEsta" name="grupEsta" readonly />
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="codEsta">Codigo</label>
                        <input type="text" value="<%= consolidacion.codigo %>" class="form-control form-control-user" id="codEsta" name="codEsta" readonly />
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="Nriesgo">Nivel de Riesgo</label>
                        <input type="text" value="<%= consolidacion.nivelRiesgo %>" class="form-control form-control-user" id="Nriesgo" name="Nriesgo" readonly />
                      </div>

                    </div>

                    <div class="form-group row">
                      <div class="col-sm-12">
                        <label class="mr-sm-2" for="tipoEsta">Tipo Del
                          Establecimiento</label>
                        <input type="text" value="<%= consolidacion.tipo %>" class="form-control form-control-user" id="tipoEsta" name="tipoEsta" readonly />
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-5">
                        <label class="mr-sm-2" for="rSocial">Razón Social</label>
                        <input type="text" value="<%= consolidacion.razonSocial %>" class="form-control form-control-user" id="rSocial" name="rSocial" readonly />
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-3" for="tIden">Tipo </label>
                        <input type="text" value="<%= consolidacion.tipoIdentificacion %>" id="tIden" name="tIden" class="form-control form-control-user" readonly>
                      </div>
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="inputIden">Identificación</label>
                        <input type="number" value="<%= consolidacion.identificacion %>" id="inputIden" name="inputIden" class="form-control form-control-user" readonly>
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="phone">Telefono</label>
                        <input type="number" value="<%= consolidacion.telefono %>" id="phone" name="phone" class="form-control form-control-user" readonly>
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-6">
                        <label class="mr-sm-2" for="rLegal">Representante Legal</label>
                        <input type="text" value="<%= consolidacion.repreLegal %>" class="form-control form-control-user" id="rLegal" name="rLegal" readonly />
                      </div>
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="direccion">Dirección</label>
                        <input type="text" value="<%= consolidacion.direccion %>" class="form-control form-control-user" id="direccion" name="direccion" readonly />
                      </div>
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="estado">Estado</label>
                        <input type="text" value="<%= consolidacion.estado %>" class="form-control form-control-user" id="estado" name="estado" readonly />
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="classVehi">Clase de Vehiculo</label>
                        <select id="classVehi" name="classVehi" class="custom-select mr-sm-2">
                          <option selected disabled>Seleccione</option>
                          <option value="MOTO">MOTO</option>
                          <option value="CAMIONETA">CAMIONETA</option>
                          <option value="CAMIÓN">CAMIÓN</option>
                          <option value="FURGON">FURGON</option>
                          <option value="FURGON">REMOLQUE</option>
                          <option value="SEMIREMOLQUE">SEMIREMOLQUE</option>
                          <option value="FURGON">FURGON</option>
                          <option value="OTRO">OTRO, ¿CUÁL?</option>
                        </select>
                      </div>
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="otroV">Otro Vehiculo</label>
                        <input type="text" class="form-control form-control-user" id="otroV" name="otroV" placeholder="¿CUÁL?" onkeyup="mayus(this);" readonly />
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="placa">Placa</label>
                        <input type="text" class="form-control form-control-user" id="placa" name="placa" value="<%= consolidacion.ForVehiculos.placa %>" onkeyup="mayus(this);" />
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="refriV">Refrigeración</label>
                        <select id="refriV" name="refriV" class="custom-select mr-sm-2">
                          <option selected disabled>Seleccione</option>
                          <option value="SI">SI</option>
                          <option value="NO">NO</option>
                        </select>
                      </div>
                      <div class="col-sm-2">
                        <label class="mr-sm-2" for="nInscrip">N° de Inscripción</label>
                        <input type="text" class="form-control form-control-user" id="nInscrip" name="nInscrip" onkeyup="mayus(this);" value="<%= consolidacion.ForVehiculos.nInscripcion %>" />
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-12">
                        <label class="mr-sm-2" for="produTrans">Productos Y/O Mercancia Que Transporta
                        </label>
                        <input type="text" class="form-control form-control-user" id="produTrans" name="produTrans" onkeyup="mayus(this);" value="<%= consolidacion.ForVehiculos.productosVehiculo %>" />
                      </div>
                    </div>

                    <datalist id="defaultProdu">
                      <option value="NO PRESENTA"></option>
                    </datalist>

                    <div class="form-group row">
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="fVisit">Fecha_Visita</label>
                        <input type="date" class="form-control form-control-user" value="<%= moment(consolidacion.fvisit).add(1, 'days').format('YYYY-MM-DD'); %>" />
                      </div>
                      <div class="col-sm-3">
                        <label class="mr-sm-2" for="score">Puntaje</label>
                        <input type="number" min="0" max="100" value="<%= consolidacion.score %>" class="form-control form-control-user" step="0.1" id="score" name="score" onkeyup="AlimentosConcepto(this);" />
                      </div>
                      <div class="col-sm-6">
                        <label class="mr-sm-2" for="concepto">Concepto</label>
                        <input type="text" class="form-control form-control-user" id="concepto" name="concepto" readonly value="NO APLICA" />
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-12">
                        <label class="mr-sm-2" for="observacion">Observaciones
                          (Opcional)</label>
                        <textarea name="observacion" id="observacion" onkeyup="mayus(this);" maxlength="270" rows="2" class="form-control form-control-user"><%= consolidacion.observaciones %></textarea>
                      </div>
                    </div>

                    <button class="btn btn-primary btn-user btn-block" type="submit">
                      Enviar Corrección
                    </button>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /.container-fluid -->
      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <%- include('../../../tools/footer'); %>
      <!-- End of Footer -->
    </div>
    <!-- End of Content Wrapper -->
  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Salida</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body">
          Seleccione "Cerrar sesión" a continuación si está listo para
          finalizar su sesión actual.
        </div>
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">
            Cancelar
          </button>
          <a class="btn btn-primary" href="/tecnico/logout">Cerrar Sesión</a>
        </div>
      </div>
    </div>
  </div>

  <!-- Bootstrap core JavaScript-->
  <script src="/vendor/jquery/jquery.min.js"></script>
  <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="/js/sb-admin-2.min.js"></script>

  <!-- Validations -->
  <script type="text/JavaScript" src="/js/tools/dropzone.js"></script>
  <script type="text/JavaScript" src="/js/Profesional/validaciones.js"></script>
  <script type="text/JavaScript" src="/js/Profesional/formularios.js"></script>

  <!-- Alert -->
  <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

  <% if(typeof alert !="undefined" ) { %>
  <script type="text/JavaScript">
    Swal.fire({
        title: '<%= alert[0].alertTitle %>',
        text:'<%= alert[0].alertMessage %>',
        icon:'<%= alert[0].alertIcon %>',
        showConfirmButton: <%= alert[0].showConfirmButton %>,
        timer: <%= alert[0].timer %>
      }).then(()=>{
        window.location = '<%= alert[0].ruta %>'
      })
    </script>
  <% } %>

  <script>
    $(window).bind('load', function() {
      var nav = document.getElementById('nav2');
      $('#navlink1').removeClass('collapsed');
      var nav3 = document.getElementById('collapseOne');
      var nav4 = document.getElementById('show1');
      nav.className += "  active";
      nav3.className += "  show";
      nav4.className += "  active";
    });
    document.getElementById("classVehi").value = "<%= consolidacion.ForVehiculos.claseVehiculo %>"
    if ("<%= consolidacion.ForVehiculos.claseVehiculo %>" == "OTRO") {
      document.getElementById("otroV").value = "<%= consolidacion.ForVehiculos.otraClase %>"
      $("#otroV").attr("readonly", false);
    }
    document.getElementById("refriV").value = "<%= consolidacion.ForVehiculos.refrigeracion %>"


    $("#classVehi").change(function() {
      if (document.getElementById("classVehi").value == "OTRO") {
        $("#otroV").attr("readonly", false);
      } else {
        document.getElementById("otroV").value = ""
        $("#otroV").attr("readonly", true);
      }
    });
  </script>
</body>

</html>