  <script>
    var rotulado = "<%= consolidacion.consolidacion.rotulado %>";
    var publicidad = "<%= consolidacion.consolidacion.publicidad %>";
    var MSEstable = "<%= consolidacion.consolidacion.MSEstablecimientos %>";
    var MSProduct = "<%= consolidacion.consolidacion.MSProductos %>";
    var Cementerio = "<%= consolidacion.salaNM %>";

    if (document.getElementById("grupEsta").value == "ACUEDUCTO") {
      document
        .getElementById("score")
        .setAttribute("onkeyup", "AcueductoConcepto(this);");
    } else if (
      document.getElementById("codEsta").value >= 1 &&
      document.getElementById("codEsta").value < 11
    ) {
      document
        .getElementById("score")
        .setAttribute("onkeyup", "AlimentosConcepto(this);");
    } else if (document.getElementById("codEsta").value >= 12) {
      document
        .getElementById("score")
        .setAttribute("onkeyup", "EstablecimientosConcepto(this);");
    }

    document.getElementById("accion").value = "<%= consolidacion.accion %>"
    document.getElementById("acta").value = "<%= consolidacion.acta %>"
    document.getElementById("actaLey").value = "<%= consolidacion.actaLey %>"

    if (rotulado === "on") {
      document.getElementById("rotuladoON").setAttribute("checked", "checked")
      document.getElementById("rotuladoTemplate").removeAttribute("style");
      document.getElementById("productoRotulado").value = "<%= consolidacion.ForRotulado.productoRotulado %>"
    }
    if (publicidad === "on") {
      document.getElementById("publicidadON").setAttribute("checked", "checked")
      document.getElementById("publicidadTemplate").removeAttribute("style");
      document.getElementById("medPubli").value = "<%= consolidacion.ForPublicidad.medioPublicitario %>"
      document.getElementById("permisoSanitario").value = "<%= consolidacion.ForPublicidad.registroSanitario %>"
      document.getElementById("productoPublicidad").value = "<%= consolidacion.ForPublicidad.productoPublicidad %>"
      document.getElementById("marcaPublicidad").value = "<%= consolidacion.ForPublicidad.marcaPublicidad %>"
    }
    if (MSEstable === "on") {
      document.getElementById("establecimientosON").setAttribute("checked", "checked")
      document.getElementById("MedEstablecimientosTemplate").removeAttribute("style");
      document.getElementById("medidaApliEstable").value = "<%= consolidacion.ForMSEstablecimientos.medidaMSEstablecimientos %>"
      document.getElementById("motivoApli").value = "<%= consolidacion.ForMSEstablecimientos.motivoMSEstablecimientos %>"
      // document.getElementById("observacionMedEsta").value = "<%= consolidacion.ForMSEstablecimientos.medidaMSEstablecimientos %>"
    }
    if (MSProduct === "on") {
      document.getElementById("productosON").setAttribute("checked", "checked")
      document.getElementById("MedProductosTemplate").removeAttribute("style");
      document.getElementById("medidaApliProduc").value = "<%= consolidacion.ForMSProductos.medidaMSProductos %>"
      document.getElementById("permisoProduco").value = "<%= consolidacion.ForMSProductos.permisoMSProductos %>"
      document.getElementById("productoMed").value = "<%= consolidacion.ForMSProductos.productoMSProductos %>"
      document.getElementById("marcaProduct").value = "<%= consolidacion.ForMSProductos.marcaMSProductos %>"
      document.getElementById("motivoProduct").value = "<%= consolidacion.ForMSProductos.motivoMSProductos %>"
      document.getElementById("presentProduct").value = "<%= consolidacion.ForMSProductos.presentacionMSProductos %>"
      document.getElementById("cantProdu").value = "<%= consolidacion.ForMSProductos.cantidadMSProductos %>"
      document.getElementById("fabriProduc").value = "<%= consolidacion.ForMSProductos.fabricanteMSProductos %>"
      document.getElementById("loteProduc").value = "<%= consolidacion.ForMSProductos.loteMSProductos %>"
      document.getElementById("fechProduc").value = "<%= moment(consolidacion.ForMSProductos.vencimientoMSProductos).format('YYYY-MM-DD'); %>"
      // $('observacionMedProd').val(<%= consolidacion.ForMSProductos.observacionMedProd %>)
      $('#observacionMedProd').val('<%= consolidacion.ForMSProductos.observacionMedProd %>');
    }
    console.log(document.getElementById("observacionMedProd").value)
    if (Cementerio != "") {
      document.getElementById("templeCementerios").removeAttribute("style");
      document.getElementById("NecroMorg").value = "<%= consolidacion.salaNM %>"
    }
  </script>
</body>

</html>