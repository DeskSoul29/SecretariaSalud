<div class="form-row align-items-center mb-sm-2">
  <div class="col my-1">
    <label class="mr-sm-2" for="provinciaID">Provincia</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.provincia %>" readonly />
  </div>
  <div class="col my-1">
    <label class="mr-sm-2" for="muniSelect">Municipio</label>
    <input id="municipio" name="municipio" type="text" class="form-control form-control-user" value="<%= consolidacion.municipio %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-8 mb-3 mb-sm-0">
    <label class="mr-sm-2" for="grupEsta">Grupo Del Establecimiento</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.grupo %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="codEsta">Codigo</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.codigo %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="Nriesgo">Nivel de Riesgo</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.nivelRiesgo %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="tipoEsta">Tipo Del Establecimiento</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.tipo %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-5">
    <label class="mr-sm-2" for="rSocial">Razón Social</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.razonSocial %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-3" for="tIden">Tipo </label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.tipoIdentificacion %>" readonly />
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="inputIden">Identificación</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.identificacion %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="phone">Telefono</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.telefono %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-6">
    <label class="mr-sm-2" for="rLegal">Representante Legal</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.repreLegal %>" readonly />
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="direccion">Dirección</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.direccion %>" readonly />
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="estado">Estado</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.hojavida.estado %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-3">
    <label class="mr-sm-2" for="classVehi">Clase de Vehiculo</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.claseVehiculo %>" readonly />
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="otroV">Otro Vehiculo</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.otraClase %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="placa">Placa</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.placa %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="refriV">Refrigeración</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.refrigeracion %>" readonly />
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="nInscrip">N° de Inscripción</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.nInscripcion %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="produTrans">Productos Y/O Mercancia Que Transporta
    </label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForVehiculos.productosVehiculo %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-3">
    <label class="mr-sm-2" for="fVisit">Fecha_Visita</label>
    <input type="text" class="form-control form-control-user" value="<%= moment(consolidacion.fvisit,"YYYY/MM/DD").add(1, 'days').format('DD-MM-YYYY'); %>" readonly />
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="score">Puntaje</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.score %>" readonly />
  </div>
  <div class="col-sm-6">
    <label class="mr-sm-2" for="concepto">Concepto</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.concepto %>" readonly />
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="observacion">Observaciones</label>
    <textarea name="observacion" id="observacion" class="form-control form-control-user" readonly><%= consolidacion.observaciones %></textarea>
  </div>
</div>