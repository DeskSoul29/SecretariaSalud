<div class="form-row align-items-center mb-sm-2">
  <div class="col my-1">
    <label class="mr-sm-2" for="provinciaID">Provincia</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.provincia %>" readonly>
  </div>
  <div class="col my-1">
    <label class="mr-sm-2" for="muniSelect">Municipio</label>
    <input id="municipio" name="municipio" type="text" class="form-control form-control-user" value="<%= consolidacion.municipio %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-4">
    <label class="mr-sm-2" for="temaCap">Tema De
      Capacitación</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.tema %>" readonly>
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="otrosCap">Otros</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.otroTema %>" readonly>
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="fechaCap">Fecha</label>
    <input type="text" class="form-control form-control-user" value="<%= moment(consolidacion.ForEduSanitaria.fechaCap,"YYYY/MM/DD").add(1, 'days').format('DD-MM-YYYY'); %>" readonly>
  </div>
  <div class="col-sm-2">
    <label class="mr-sm-2" for="intensidadCap">Intensidad(Horas)</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.intensidad %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col my-1">
    <label class="mr-sm-2" for="lugCap">Lugar de
      Capacitación</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.lugarCapa %>" readonly>
  </div>
  <div class="col my-1">
    <label class="mr-sm-2" for="personalCap">Personal
      Dirigido</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.personalDiri %>" readonly>
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="totalPersCap">Total Personal
      Capacitado</label>
    <input type="number" class="form-control form-control-user" value="<%= consolidacion.ForEduSanitaria.totalPersCap %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="observacion">Observaciones</label>
    <textarea name="observacion" id="observacion" class="form-control form-control-user" readonly><%= consolidacion.observaciones %></textarea>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="fields">Archivo(s) Adjuntos</label>
    <div class="control-group" id="fields">
      <div class="controls">
        <div class="entry input-group upload-input-group">
          <input class="form-control" name="myFiles" id="myfiles" type="file" />
          <button class="btn btn-upload btn-success btn-add" type="button">
            <i class="fa fa-plus"></i>
          </button>
        </div>
      </div>
    </div>
  </div>
</div>