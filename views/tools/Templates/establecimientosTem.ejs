<div class="form-row align-items-center mb-sm-2">
    <div class="col my-1">
        <label class="mr-sm-2" for="provinciaID">Provincia</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.provincia %>" readonly>
    </div>
    <div class="col my-1">
        <label class="mr-sm-2" for="muniSelect">Municipio</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.municipio %>" readonly>
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-8 mb-3 mb-sm-0">
        <label class="mr-sm-2" for="grupEsta">Grupo Del
            Establecimiento</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.grupo %>" readonly>
    </div>
    <div class="col-sm-2">
        <label class="mr-sm-2" for="codEsta">Codigo</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.codigo %>" readonly />
    </div>
    <div class="col-sm-2">
        <label class="mr-sm-2" for="Nriesgo">Nivel de Riesgo</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.nivelRiesgo %>" readonly />
    </div>

</div>

<div class="form-group row">
    <div class="col-sm-12">
        <label class="mr-sm-2" for="tipoEsta">Tipo Del
            Establecimiento</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.tipo %>" readonly />
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-5">
        <label class="mr-sm-2" for="rSocial">Razón Social</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.razonSocial %>" readonly />
    </div>
    <div class="col-sm-2">
        <label class="mr-sm-3" for="tIden">Tipo </label>
        <input type="text" value="<%= consolidacion.tipoIdentificacion %>" class="form-control form-control-user"
            readonly>
    </div>
    <div class="col-sm-3">
        <label class="mr-sm-2" for="inputIden">Identificación</label>
        <input type="number" value="<%= consolidacion.identificacion %>" class="form-control form-control-user"
            readonly>
    </div>
    <div class="col-sm-2">
        <label class="mr-sm-2" for="phone">Telefono</label>
        <input type="number" value="<%= consolidacion.telefono %>" class="form-control form-control-user" readonly>
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-6">
        <label class="mr-sm-2" for="rLegal">Representante Legal</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.repreLegal %>" readonly />
    </div>
    <div class="col-sm-3">
        <label class="mr-sm-2" for="direccion">Dirección</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.direccion %>" readonly />
    </div>
    <div class="col-sm-3">
        <label class="mr-sm-2" for="estado">Estado</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.estado %>" readonly />
    </div>
</div>
<div class="form-group row">
    <div class="col-sm-3">
        <label class="mr-sm-2" for="fVisit">Fecha_Visita</label>
        <input type="text" class="form-control form-control-user" value="<%= consolidacion.fvisit %>" readonly />
    </div>
    <div class="col-sm-3">
        <label class="mr-sm-2" for="score">Puntaje</label>
        <input type="number" class="form-control form-control-user" value="<%= consolidacion.score %>" readonly />
    </div>
    <div class="col-sm-6">
        <label class="mr-sm-2" for="concepto">Concepto</label>
        <input type="text" class="form-control form-control-user" readonly value="<%= consolidacion.concepto %>" />
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-4">
        <label class="mr-sm-2" for="accion">Acción Realizada</label>
        <input type="text" class="form-control form-control-user" readonly value="<%= consolidacion.accion %>" />
    </div>
    <div class="col-sm-8">
        <label class="mr-sm-2" for="acta">Tipo de Acta
            Utilizada</label>
        <input type="text" class="form-control form-control-user" readonly value="<%= consolidacion.acta %>" />
    </div>
</div>

<div class="form-group row" id="extraEstablecimiento">
    <div class="col-sm-4">
        <label class="mr-sm-2" for="actaLey">¿Aplico Acta De La Ley 1335?</label>
        <input type="text" class="form-control form-control-user" readonly value="<%= consolidacion.actaLey %>" />
    </div>

    <div id="templeCementerios" class="col-sm-8">
        <label class="mr-sm-3" for="NecroMorg">¿El Cementerio Cuenta con
            Sala de Necropsia o
            Morgue?</label>
        <input type="text" class="form-control form-control-user" readonly value="<%= consolidacion.salaNM %>" />
    </div>

    <div class="col-sm-4" id="extraEstablecimientoIVC" style="display: none">
        <label class="mr-sm-2">Acciones IVC Adicional</label>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="rotuladoON" name="rotuladoON" />
            <label class="form-check-label" for="rotuladoON">ROTULADO</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="publicidadON" name="publicidadON" />
            <label class="form-check-label" for="publicidadON">PUBLICIDAD</label>
        </div>
    </div>
    <div class="col-sm-4" id="extraEstablecimientoMD" style="display: none">
        <label class="mr-sm-2">Medidas Sanitarias</label>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="establecimientosON" name="establecimientosON" />
            <label class="form-check-label" for="establecimientosON">ESTABLECIMIENTOS</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="productosON" name="productosON" />
            <label class="form-check-label" for="productosON">PRODUCTOS</label>
        </div>
    </div>

</div>

<div class="form-group row">
    <div class="col-sm-12">
        <label class="mr-sm-2" for="observacion">Observaciones</label>
        <textarea name="observacion" id="observacion" class="form-control form-control-user"
            value="<%= consolidacion.observaciones %>" readonly></textarea>
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-12">
        <label class="mr-sm-2" for="fields">Archivo(s) Adjuntos</label>
        <div class="control-group" id="fields">
            <div class="controls">
                <div class="entry input-group upload-input-group">
                    <input class="form-control" name="myFiles" id="myfiles" type="file" />
                    <button class="btn btn-upload btn-success btn-add" type="button">
                        <i class="fa fa-plus"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>