<div class="form-row align-items-center mb-sm-2">
  <div class="col my-1">
    <label class="mr-sm-2" for="provinciaID">Provincia</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.provincia %>" readonly>
  </div>
  <div class="col my-1">
    <label class="mr-sm-2" for="muniSelect">Municipio</label>
    <input id="municipio" name="municipio" type="text" class="form-control form-control-user" value="<%= consolidacion.municipio %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-3">
    <label class="mr-sm-2" for="expCarnet">Expedición del
      Carnet</label>
    <input type="text" class="form-control form-control-user" value="<%= moment(consolidacion.ForCarnets.expCarnet,"YYYY/MM/DD").add(1, 'days').format('DD-MM-YYYY'); %>" readonly>
  </div>
  <div class="col-sm-3">
    <label class="mr-sm-2" for="idenCarnet">Identificación</label>
    <input type="number" class="form-control form-control-user" value="<%= consolidacion.ForCarnets.idenCarnet %>" readonly>
  </div>
  <div class="col-sm-6">
    <label class="mr-sm-2" for="nameCarnet">Nombre Completo</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForCarnets.nombreCarnet %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-7">
    <label class="mr-sm-2" for="estableciCarnet">Establecimiento
      Donde
      Labora</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForCarnets.establecimientoCarnet %>" readonly>
  </div>
  <div class="col-sm-5">
    <label class="mr-sm-2" for="direcCarnet">Dirección del
      Establecimiento</label>
    <input type="text" class="form-control form-control-user" value="<%= consolidacion.ForCarnets.direccionCarnet %>" readonly>
  </div>
</div>

<div class="form-group row">
  <div class="col-sm-12">
    <label class="mr-sm-2" for="observacion">Observaciones</label>
    <textarea name="observacion" id="observacion" class="form-control form-control-user" readonly><%= consolidacion.observaciones %></textarea>
  </div>
</div>