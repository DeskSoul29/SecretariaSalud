<!DOCTYPE html>
<html lang="es">

<head>
  <%- include('../tools/head'); %>
  <!-- Custom styles for this page -->
  <link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
  <link rel="stylesheet" href="/css/dropzone.css">
</head>

<body id="page-top">
  <!-- Page Wrapper -->
  <div id="wrapper">
    <!-- Sidebar -->
    <%- include('../tools/sidebarProf'); %>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <%- include('../tools/topbar'); %>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">
              Bienvenido, <%= user.nombre %>
              <%= user.apellido %>
            </h1>
          </div>

          <!-- Content Row -->
          <div class="form-row align-items-center mb-sm-2">
            <!-- Consolidaciones Pendientes -->
            <div class="col my-1">
              <div class="card border-left-warning shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-warning text-uppercase mb-1">
                        Consolidaciones Pendientes
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consPend %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-exclamation fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Conrregidas -->
            <div class="col my-1">
              <div class="card border-left-primary shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                        Consolidaciones Corregidas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consCorre %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-pen fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Enviadas -->
            <div class="col my-1">
              <div class="card border-left-info shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-info text-uppercase mb-1">
                        Consolidaciones Enviadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consEnv %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-export fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Rechazadas -->
            <div class="col my-1">
              <div class="card border-left-danger shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-danger text-uppercase mb-1">
                        Consolidaciones Rechazadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consRechProf %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-xmark fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Aceptadas -->
            <div class="col my-1">
              <div class="card border-left-success shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                        Consolidaciones Aceptadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consAcep %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-check fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-xl-9">
              <div class="card shadow mb-4">

                <div id="accordion">
                  <!-- Consolidaciones Pendientes -->
                  <div class="card">
                    <div class="card-header" id="headingOne">
                      <h5 class="mb-0">
                        <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                          Consolidaciones Pendientes
                        </button>
                      </h5>
                    </div>
                    <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                      <div class="card-body">
                        <div class="table-responsive">
                          <table class="table table-bordered table-hover display nowrap" id="dataTable" width="100%" cellspacing="0" style="font-size: 13px;  text-align: center !important;">
                            <thead>
                              <tr>
                                <th>Estado</th>
                                <th>Fecha_Enviado</th>
                                <th>Tipo Consolidación</th>
                                <th>Municipio</th>
                                <th>Responsable</th>
                              </tr>
                            </thead>
                            <tfoot>
                              <tr>
                                <th>Estado</th>
                                <th>Fecha_Enviado</th>
                                <th>Tipo Consolidación</th>
                                <th>Municipio</th>
                                <th>Responsable</th>
                              </tr>
                            </tfoot>
                            <tbody>
                              <% if (allConso){ %>
                              <% allConso.forEach((allConso)=> { %>
                              <% if(allConso.status=="Pendiente" && allConso.consolidacion.tomaMuestra!="on" && allConso.consolidacion.vivienda != "on"){ %>
                              <tr>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/Establecimientos/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/AntirrabicaAnimal/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/EduSanitaria/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/EventosSaludPublica/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/ListadoCarnetizados/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/Vehiculos/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/Quejas/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.cronograma=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/Cronograma/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.viviSalu=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/ViviendaSaludable/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.permMunicipio=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/PermMunicipio/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } else if(allConso.consolidacion.alertSani=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Validar/AlertaSanitaria/<%= allConso._id %>"><span class="badge badge-warning text-uppercase">Pendiente</span></a>
                                  <% } %>
                                </td>
                                <td>
                                  <%= moment(allConso.createdAt,"YYYY/MM/DD").format('YYYY-MM-DD'); %>
                                </td>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  Establecimientos
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  Antirrabica Animal
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  Educación Sanitaria
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  Eventos de Salud Publica
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  Listado de Carnetizados
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  Vehiculos
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  Toma de Muestras
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  Quejas
                                  <% } else if(allConso.consolidacion.cronograma=="on" ){ %>
                                  Cronograma de Actividades
                                  <% } else if(allConso.consolidacion.viviSalu=="on" ){ %>
                                  Vivienda Saludable
                                  <% } else if(allConso.consolidacion.permMunicipio=="on" ){ %>
                                  Permanencia en Municipio
                                  <% } else if(allConso.consolidacion.alertSani=="on" ){ %>
                                  Alerta Sanitaria
                                  <% } %>
                                </td>
                                <td>
                                  <%= allConso.municipio %>
                                </td>
                                <td>
                                  <%= allConso.responsable.userResponsable %> -
                                  <%= allConso.responsable.nombreResponsable %>
                                </td>
                              </tr>
                              <% } %>
                              <% }); %>
                              <% } %>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>

