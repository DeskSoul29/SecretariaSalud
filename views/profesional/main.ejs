                                <th>Fecha_Corregido</th>
                                <th>Tipo Consolidación</th>
                                <th>Municipio</th>
                                <th>Responsable</th>
                              </tr>
                            </tfoot>
                            <tbody>
                              <% if (allConso){ %>
                              <% allConso.forEach((allConso)=> { %>
                              <% if(allConso.status=="Corregido" ){ %>
                              <tr>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Establecimientos/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/AntirrabicaAnimal/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/EduSanitaria/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/EventosSaludPublica/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/ListadoCarnetizados/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Vehiculos/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/TomaMuestras/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Quejas/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span> </a>
                                  <% } %>
                                </td>
                                <td>
                                  <%= moment(allConso.reporte.fechRepor,"YYYY/MM/DD").format('DD-MM-YYYY h:mma'); %>
                                </td>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  Establecimientos
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  Antirrabica Animal
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  Educación Sanitaria
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  Eventos de Salud Publica
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  Listado de Carnetizados
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  Vehiculos
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  Toma de Muestras
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  Quejas
                                  <% } %>
                                </td>
                                <td>
                                  <%= allConso.municipio %><%= allConso.ForQuejas.municipioComuni %>
                                </td>
                                <td>
                                  <%= allConso.responsable.userResponsable %> -
                                  <%= allConso.responsable.nombreResponsable %>
                                </td>
                              </tr>
                              <% } %>
                              <% }); %>
                              <% } %>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="card">
                    <div class="card-header" id="headingThree">
                      <h5 class="mb-0">
                        <button class="btn btn-link collapsed" data-toggle="collapse" data-target="#collapse3" aria-expanded="false" aria-controls="collapse3">
                          Consolidaciones Rechazadas Propias
                        </button>
                        </h2>
                    </div>
                    <div id="collapse3" class="collapse" aria-labelledby="headingThree" data-parent="#accordion">
                      <div class="card-body">
                        <div class="table-responsive">
                          <table class="table table-bordered table-hover display nowrap" id="dataTable3" width="100%" cellspacing="0" style="font-size: 13px;  text-align: center !important;">
                            <thead>
                              <tr>
                                <th>Estado</th>
                                <th>Fecha_Rechazado</th>
                                <th>Tipo Consolidación</th>
                                <th>Municipio</th>
                                <th>Responsable</th>
                              </tr>
                            </thead>
                            <tfoot>
                              <tr>
                                <th>Estado</th>
                                <th>Fecha_Rechazado</th>
                                <th>Tipo Consolidación</th>
                                <th>Municipio</th>
                                <th>Responsable</th>
                              </tr>
                            </tfoot>
                            <tbody>
                              <% if (allConso){ %>
                              <% allConso.forEach((allConso)=> { %>
                              <% if(allConso.status=="Rechazado" && allConso.responsable.userResponsable == user.user){ %>
                              <tr>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/Establecimientos/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/AntirrabicaAnimal/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/EduSanitaria/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/EventosSaludPublica/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/ListadoCarnetizados/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/Vehiculos/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/TomaMuestras/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/Quejas/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } else if(allConso.consolidacion.noveadministrativa=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Rechazado/NoveAdministrativa/<%= allConso._id %>"><span class="badge badge-danger text-uppercase">Rechazado</span></a>
                                  <% } %>
                                </td>
                                <td>
                                  <%= moment(allConso.reporte.fechRepor,"YYYY/MM/DD").format('DD-MM-YYYY h:mma'); %>
                                </td>
                                <td>
                                  <% if(allConso.consolidacion.establecimiento=="on" ){ %>
                                  Establecimientos
                                  <% } else if(allConso.consolidacion.antirrabica=="on" ){ %>
                                  Antirrabica Animal
                                  <% } else if(allConso.consolidacion.eduSanitaria=="on" ){ %>
                                  Educación Sanitaria
                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  Eventos de Salud Publica
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  Listado de Carnetizados
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  Vehiculos
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  Toma de Muestras
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  Quejas
                                  <% } else if(allConso.consolidacion.noveadministrativa=="on" ){ %>
                                  Novedad Administrativa
                                  <% } %>
                                </td>
                                <td>
                                  <%= allConso.municipio %><%= allConso.ForQuejas.municipioComuni %>
                                </td>
                                <td>
                                  <%= allConso.responsable.userResponsable %> -
                                  <%= allConso.responsable.nombreResponsable %>
                                </td>
                              </tr>
                              <% } %>
                              <% }); %>
                              <% } %>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>
                  </div>

                </div>
              </div>
            </div>

            <div class="col-xl-3">
              <ul class="list-group">
                <li class="list-group-item d-flex justify-content-between align-items-center">
                  Vacunaciones
                  <span class="badge badge-primary badge-pill"><%= vacunas %></span>
                </li>
                <li class="list-group-item d-flex justify-content-between align-items-center">
                  Total Visitas
                  <span class="badge badge-primary badge-pill"><%= visitAcep %></span>
                </li>
              </ul>
            </div>

          </div>

        </div>
        <!-- /.container-fluid -->
      </div>
      <!-- End of Main Content -->

      <!-- Footer -->
      <%- include('../tools/footer'); %>
      <!-- End of Footer -->
    </div>
    <!-- End of Content Wrapper -->
  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Nov Administrativa Modal -->
  <div class="modal fade" id="NovAdModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class=" modal-dialog" role="document" style="max-width: 700px !important;">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title"><i class="fa-solid fa-triangle-exclamation"> Atencion</i></h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <img src="/img/6.svg" class="d-block w-100">
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">
            Cerrar
          </button>
          <a class="btn btn-primary" href="/profesional/Consolidaciones/Enviar/NoveAdministrativa">Vamos</a>
        </div>
      </div>
    </div>
  </div>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Salida</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body">
          Seleccione "Cerrar sesión" a continuación si está listo para
          finalizar su sesión actual.
        </div>
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">
            Cancelar
          </button>
          <a class="btn btn-primary" href="/profesional/logout">Cerrar Sesión</a>
        </div>
      </div>
    </div>
  </div>

  <!-- Bootstrap core JavaScript-->
