<!DOCTYPE html>
<html lang="es">

<head>
  <%- include('../tools/head'); %>
  <!-- Custom styles for this page -->
  <link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/amcharts/3.21.15/plugins/export/libs/FileSaver.js/FileSaver.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.15.6/xlsx.full.min.js"></script>
  <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.2.3/css/buttons.dataTables.min.css">
</head>

<body id="page-top">
  <!-- Page Wrapper -->
  <div id="wrapper">
    <!-- Sidebar -->
    <%- include('../tools/sidebarProf'); %>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <%- include('../tools/topbar'); %>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">
              Bienvenido, <%= user.nombre %>
              <%= user.apellido %>
            </h1>
          </div>

          <!-- Content Row -->
          <div class="form-row align-items-center mb-sm-2">
            <!-- Consolidaciones Pendientes -->
            <div class="col my-1">
              <div class="card border-left-warning shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-warning text-uppercase mb-1">
                        Consolidaciones Pendientes
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consPend %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-exclamation fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Conrregidas -->
            <div class="col my-1">
              <div class="card border-left-primary shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">
                        Consolidaciones Corregidas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consCorre %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-pen fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Enviadas -->
            <div class="col my-1">
              <div class="card border-left-info shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-info text-uppercase mb-1">
                        Consolidaciones Enviadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consEnv %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-export fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Rechazadas -->
            <div class="col my-1">
              <div class="card border-left-danger shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-danger text-uppercase mb-1">
                        Consolidaciones Rechazadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consRechProf %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-xmark fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Consolidaciones Aceptadas -->
            <div class="col my-1">
              <div class="card border-left-success shadow h-100 py-2">
                <div class="card-body">
                  <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                      <div class="text-xs font-weight-bold text-success text-uppercase mb-1">
                        Consolidaciones Aceptadas
                      </div>
                      <div class="h5 mb-0 font-weight-bold text-gray-800">
                        <%= consAcep %>
                      </div>
                    </div>
                    <div class="col-auto">
                      <i class="fas fa-file-circle-check fa-2x"></i>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-xl-9">
              <div class="card shadow mb-4">

                <div id="accordion">

                  <div class="card">
                    <div class="card-header" id="headingOne">
                      <h5 class="mb-0">
                        <button class="btn btn-link" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                          Consolidaciones Pendientes
                        </button>
                      </h5>
                    </div>
                    <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                      <div class="card-body">
                        <div class="table-responsive">
