                                  <% } else if(allConso.consolidacion.EvenSaludPubli=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/EventosSaludPublica/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.lisCarnets=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/ListadoCarnetizados/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.vehiculos=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Vehiculos/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.tomaMuestra=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/TomaMuestras/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.quejas=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Quejas/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span> </a>
                                  <% } else if(allConso.consolidacion.cronograma=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/Cronograma/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.viviSalu=="on" ){ %>
                                  <a href="/profesional/Consolidaciones/Correccion/ViviendaSaludable/<%= allConso._id %>"><span class="badge badge-primary text-uppercase">Corregido</span></a>
                                  <% } else if(allConso.consolidacion.permMunicipio=="on" ){ %>
